{
  "nixEnvSelector.nixFile": "${workspaceRoot}/shell.nix",
  "rust-analyzer.cargo.features": "all",
  "markdownlint.config": {
    "commands-show-output": false,
    "no-duplicate-header": false
  },
  "cSpell.enabled": true,
  "cSpell.words": [
    "BINPATH",
    "canonicalize",
    "Clippy",
    "factorio",
    "indoc",
    "itertools",
    "Localised",
    "MSRV",
    "pastable",
    "printdoc",
    "rustfmt",
    "rustup",
    "serde",
    "Swatinem",
    "tempdir",
    "tempfile",
    "thiserror",
    "writedoc"
  ]
}
